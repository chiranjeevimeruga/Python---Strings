     "text": [
      "Enter first String:Chiranjeevi\n",
      "Enter second String:Meruga\n",
      "The Common Letters are:\n",
      "r\n",
      "a\n",
      "e\n"
     ]
    }
   ],
   "source": [
    "s1 = input(\"Enter first String:\")\n",
    "\n",
    "s2 = input(\"Enter second String:\")\n",
    "\n",
    "a = list(set(s1) & set(s2))\n",
    "print(\"The Common Letters are:\")\n",
    "for i in a:\n",
    "    print(i)"
